block('m-vmenu')(
	tag()('nav'),
	content()(function () {
		return [
			this.ctx.content.map(function(item) {
				return {
					block: 'm-vmenu-item',
					mods: item.state ? { state : item.state } : null,
					content: item.title,
					url: item.url
				}
			})
		]
	}),
	match(this._done)(
		content()(function () {
			this._done = true;
			return {
				elem: 'panel',
				tag: 'ul',
				content: applyNext()
			}
		})
	)
)