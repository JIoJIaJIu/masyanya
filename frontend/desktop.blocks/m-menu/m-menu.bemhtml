block('m-menu')(
    js()(true),
    tag()('ul'),
    content()(function () {
        return [
            this.ctx.content.map(function(item) {
                return {
                    elem: 'item',
                    tag: 'li',
                    content: {
                        block: 'm-link',
                        url: item.url,
                        content: item.title
                    }
                } 
            })
        ]
    })
)
