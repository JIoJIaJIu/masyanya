block('m-heading').match(function () { return this.ctx.anchor; })(
    attrs()(function () {
        return {
            id: this.ctx.anchor
        }
    })
)

block('m-heading').mod('type', 'main')(
    tag()('h1')
)

block('m-heading').mod('size', 'px26')(
    tag()('h2')
)

block('m-heading').mod('type', 'subsection')(
    tag()('h3')
)